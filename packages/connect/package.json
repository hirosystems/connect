{
  "name": "@stacks/connect",
  "version": "7.3.0",
  "license": "MIT",
  "prettier": "@stacks/prettier-config",
  "scripts": {
    "build": "concurrently 'tsup src/index.ts' 'yarn types'",
    "prepublishOnly": "yarn build",
    "typecheck": "tsc --project tsconfig.json --noEmit",
    "types": "tsc --project tsconfig.json --emitDeclarationOnly"
  },
  "dependencies": {
    "@stacks/auth": "^6.1.1",
    "@stacks/connect-ui": "6.1.0",
    "@stacks/network": "^6.1.1",
    "@stacks/profile": "^6.1.1",
    "@stacks/transactions": "^6.1.1",
    "jsontokens": "^4.0.1"
  },
  "sideEffects": false,
  "publishConfig": {
    "access": "public"
  },
  "files": [
    "dist"
  ],
  "typings": "dist/types/index.d.ts",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "umd:main": "dist/index.global.js",
  "unpkg": "dist/index.global.js",
  "browser": {
    "crypto": false
  },
  "typedoc": {
    "entryPoint": "./src/index.ts"
  }
}
